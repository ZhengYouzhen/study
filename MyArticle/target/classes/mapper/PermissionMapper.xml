<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace填写mapper映射文件关联的dao接口类（全限定名） -->
<mapper namespace="com.zyz.dao.PermissionMapper">

    <insert id="save" parameterType="com.zyz.bean.Permission">
        <![CDATA[
            insert into t_permission(permission, des_zh) values(#{permission}, #{desZh})
        ]]>
    </insert>

</mapper>